{
	"Arrow Function": {
		"prefix": "afn",
		"body": [
			"($1) => $2"
		],
		"description": "Arrow function"
	},
	"useAppDispath": {
		"prefix": "useAppDispath",
		"body": [
			"const dispath = useAppDispatch();"
		]
	},
	"useAppSelector": {
		"prefix": "useAppSelector",
		"body": [
			"const $1 = useAppSelector(state => $2);"
		]
	},
	"Export Default Component": {
		"prefix": [
			"exdc"
		],
		"body": [
			"import { type Component } from \"solid-js\";",
			"const ${1:Component}: Component = () => {",
			"\treturn <>$2</>;",
			"};",
			"export default ${1:Component};"
		]
	},
	"Export Default Component props": {
		"prefix": [
			"exdcp"
		],
		"body": [
			"import { type Component } from \"solid-js\";",
			"const ${1:Component}: Component<$2> = (props) => {",
			"\treturn <>$3</>;",
			"};",
			"export default ${1:Component};"
		]
	},
	"Solid Component": {
		"prefix": [
			"comp"
		],
		"body": [
			"const ${1:Component}: Component = (props) => {",
			"\treturn <>$2</>;",
			"};"
		]
	},
	"Create Signal": {
		"prefix": [
			"createSignalSnippet",
			"cs"
		],
		"body": "const [${1:signal}, set${1/(.*)/${1:/capitalize}/}] = createSignal($2)"
	},
	"useNavigate": {
		"prefix": "useNavigateSnippet",
		"body": "const navigate = useNavigate();"
	},
	"createEffectSnippet": {
		"prefix": "createEffectSnippet",
		"body": [
			"createEffect(() => {",
			"\t$1",
			"})"
		]
	},
	"useQuery": {
		"prefix": "useQuerySnippet",
		"body": [
			"const ${1:query} = useQuery(()=>({",
			"\tqueryKey: [\"$2\"],",
			"\tqueryFn: $3",
			"}))"
		]
	}
}
